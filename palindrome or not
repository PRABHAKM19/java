public class rev{
    public static void main(String args[]){
        String a="mono";
        String rev="";

        for (int i=a.length()-1;i>=0;i--){
            rev+=a.charAt(i);
        }
        if (a.equals(rev)){
            System.out.println("Yes!,it is palindrome");
        }else{
            System.out.println("No! it is not palindome");

        }

    }
}
