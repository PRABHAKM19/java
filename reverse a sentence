public class rev{
    public static void main(String args[]){
        String a="prabha is an AI&DS student";
        String rev="";

        for (int i=a.length()-1;i>=0;i--){
            rev+=a.charAt(i);
        }

        System.out.println("org str:"+a);
        System.out.println("reversed str:"+rev);

    }
}
