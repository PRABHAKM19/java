class hello
{
    public static void main(String args[])
    {
        int a=40;
        int b=40;
        if (a<b){
            System.out.print("a is greater than b");
        }else if(a==b){
            System.out.print("a is equal to b");
        }else{
            System.out.print("a is smaller than b");
        }
        }
    }
